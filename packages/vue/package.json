{
  "name": "@blockgain/livekit-vue",
  "version": "0.1.0",
  "type": "module",
  "license": "Apache-2.0",
  "author": "Blockgain",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./composables": {
      "types": "./dist/composables.d.ts",
      "import": "./dist/composables.js",
      "require": "./dist/composables.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepare": "vite build",
    "lint": "eslint -f unix \"src/**/*.{ts,vue}\"",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@livekit/components-core": "^0.12.10",
    "@livekit/components-styles": "^1.1.6",
    "rxjs": "7.8.2"
  },
  "peerDependencies": {
    "livekit-client": "^2.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^26.0.0",
    "rimraf": "^6.0.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.2.3",
    "vitest": "^3.0.0",
    "vue-tsc": "^3.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
